#
# The following are used by the outlook express import code to display status/error
# and informational messages
#
# Short name of import module
## @name TEXTIMPORT_NAME
## @loc None
# # @name TEXTIMPORT_NAME
# # @loc None
#. #
#. # The following are used by the outlook express import code to display status/error 
#. # and informational messages
#. #
#. # Short name of import module
#. ## @name TEXTIMPORT_NAME
#. ## @loc None
#: 2000
msgid "Text file (LDIF, .tab, .csv, .txt)"
msgstr "Текст (LDIF, .tab, .csv, .txt)"

# Description of import module
## @name TEXTIMPORT_DESCRIPTION
## @loc None
# # @name TEXTIMPORT_DESCRIPTION
# # @loc None
#. # Description of import module
#. ## @name TEXTIMPORT_DESCRIPTION
#. ## @loc None
#: 2001
msgid ""
"Import an address book from a text file, including:  LDIF (.ldif, .ldi), "
"tab-delimited (.tab, .txt) or comma-separated (.csv) formats."
msgstr ""
"Увези адресар из текстуалне датотеке, укључујући::  LDIF (.ldif, .ldi), "
"табулаторски формат (.tab, .txt) или формат са запетама (.csv)."

# Description of import module
## @name TEXTIMPORT_ADDRESS_NAME
## @loc None
# # @name TEXTIMPORT_ADDRESS_NAME
# # @loc None
#. # Description of import module
#. ## @name TEXTIMPORT_ADDRESS_NAME
#. ## @loc None
#: 2002
msgid "Text Address Book"
msgstr "Текстуални адресар"

# Description
## @name TEXTIMPORT_ADDRESS_SUCCESS
## @loc None
# # @name OEIMPORT_ADDRESS_SUCCESS
# # @loc None
# # LOCALIZATION NOTE (2006): In this item, don't translate "%S"
# # The variable %S will receive the name of the address book
#. # Description
#. ## @name TEXTIMPORT_ADDRESS_SUCCESS
#. ## @loc None
#: 2003
msgid "Imported address book %S"
msgstr "Увезен је адресар %S"

# Error message
## @name TEXTIMPORT_ADDRESS_BADPARAM
## @loc None
# # @name TEXTIMPORT_ADDRESS_BADPARAM
# # @loc None
#. # Error message
#. ## @name TEXTIMPORT_ADDRESS_BADPARAM
#. ## @loc None
#: 2004
msgid "Bad parameter passed to import address book."
msgstr "Параметар прослеђен увознику адресара је неисправан."

# Error message
## @name TEXTIMPORT_ADDRESS_BADSOURCEFILE
## @loc None
# # @name TEXTIMPORT_ADDRESS_BADSOURCEFILE
# # @loc None
#. # Error message
#. ## @name TEXTIMPORT_ADDRESS_BADSOURCEFILE
#. ## @loc None
#: 2005
msgid "Error accessing file for address book %S."
msgstr "Дошло је до грешке при приступању датотеци са адресаром %S."

# Error message
## @name TEXTIMPORT_ADDRESS_CONVERTERROR
## @loc None
# # @name OEIMPORT_ADDRESS_CONVERTERROR
# # @loc None
# # LOCALIZATION NOTE (2009): In this item, don't translate "%S"
# # The variable %S will receive the name of the address book
#. # Error message
#. ## @name TEXTIMPORT_ADDRESS_CONVERTERROR
#. ## @loc None
#: 2006
msgid ""
"Error importing address book %S, all addresses may not have been imported."
msgstr ""
"Десила се грешка при увозу адресара %S, могуће је да нису увезене све "
"адресе."
