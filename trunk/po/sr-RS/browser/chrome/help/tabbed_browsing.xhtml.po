msgstr ""

#: None:27
#, fuzzy
msgid "To bookmark a set of tabs:"
msgstr ""

#: None:28
#, fuzzy
msgid "<ol>"
msgstr ""

#: None:29
#, fuzzy
msgid "<span class=\"noUnix\">Press &accelKey;+&shiftKey;+<kbd>D</kbd> or        select</span><span class=\"unix\">Select</span>        <span class=\"menuPath\">Bookmarks &gt; Bookmark All Tabs</span> to add        all the tabs in the current window to a new bookmark folder."
