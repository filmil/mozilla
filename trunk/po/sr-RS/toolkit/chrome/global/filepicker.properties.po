msgid "Confirm"
msgstr "Потврди"

#: confirmFileReplacing
msgid ""
"%S already exists.\n"
"Do you want to replace it?"
msgstr ""
"%S већ постоји. \n"
"Да га заменим?"

#: openButtonLabel
msgid "Open"
msgstr "Отвори"

#: saveButtonLabel
msgid "Save"
msgstr "Сачувај"

#: selectFolderButtonLabel
msgid "Select"
msgstr "Изабери"

#: noButtonLabel
msgid "No"
msgstr "Не"

#: saveAsLabel
msgid "Save As:"
msgstr ""

#: errorOpenFileDoesntExistTitle errorOpeningFileTitle
msgid "Error opening %S"
msgstr "Грешка при отварању %S"

#: errorOpenFileDoesntExistMessage
msgid "File %S doesn't exist"
msgstr "Датотека %S не постоји"

#: errorDirDoesntExistTitle
msgid "Error accessing %S"
msgstr "Грешка у приступу %S"

#: errorDirDoesntExistMessage
msgid "Directory %S doesn't exist"
msgstr "Директоријум %S не постоји"

#: openWithoutPermissionMessage_file
msgid "File %S is not readable"
msgstr "Не могу да прочитам датотеку %S"

#: errorSavingFileTitle
msgid "Error saving %S"
msgstr "Грешка при чувању %S"

#: saveParentIsFileMessage
msgid "%S is a file, can't save %S"
msgstr "%S је датотека, не могу да сачувам %S"

#: saveParentDoesntExistMessage
msgid "Path %S doesn't exist, can't save %S"
msgstr "Путања %S не постоји, не могу да сачувам %S"

#: saveWithoutPermissionMessage_file
msgid "File %S is not writable."
msgstr "Датотека %S је недоступна за упис."

#: saveWithoutPermissionMessage_dir
msgid "Cannot create file. Directory %S is not writable."
msgstr "Не могу да направим датотеку. Директоријум %S је недоступан за упис."

#: errorNewDirDoesExistTitle errorCreateNewDirTitle
msgid "Error creating %S"
msgstr "Грешка при стварању %S"

#: errorNewDirDoesExistMessage
msgid "A file named %S already exists, directory cannot be created."
msgstr ""
"Датотека са именом %S већ постоји, директоријум не може бити направљен."

#: errorCreateNewDirMessage
msgid "Directory %S could not be created"
msgstr "Директоријум %S не може бити направљен"

#: errorCreateNewDirIsFileMessage
msgid "Directory cannot be created, %S is a file"
msgstr "Директоријум не може бити направљен, %S - датотека"

#: errorCreateNewDirPermissionMessage
msgid "Directory cannot be created, %S not writable"
msgstr "Није могуће направити директоријум јер је забрањено писање у %S"

#: promptNewDirTitle
msgid "Create new directory"
msgstr "Направи нови директоријум"

#: errorPathProblemTitle
msgid "Unknown Error"
msgstr "Непозната грешка"

#: errorPathProblemMessage
msgid "An unknown error occured (path %S)"
msgstr "Појавила се непозната грешка (путања %S)"

#~ msgid "Directory %S is not readable or does not exist."
#~ msgstr "Директоријум %S није читљив или не постоји."
