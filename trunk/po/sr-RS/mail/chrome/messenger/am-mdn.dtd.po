# LOCALIZATION NOTE pref-receipts.dtd: UI for Mail/News Return Receipts prefs
#: pane.title
msgid "Return Receipts"
msgstr "Потврде о пријему"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "&Користи моја глобална подешавања за потврде о пријему за овај налог"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "&Постави посебна подешавања за потврде за овај налог"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&При слању порука, увек тражи потврду о пријему"

# LOCALIZATION NOTE dsn.label: don't translate "DSN"
#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Када потврда стигне:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "Остави у улазном &сандучету"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "&Помери у сандуче „Послато“"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Када примим захтев за потврдом о пријему:"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "Дозволи слање по&тврда за неке поруке"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "&Никад не шаљи потврду"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "Ако нисам у пољима &За и Коп поруке:"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "Ако је пошиљалац ван мог домена:"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "У свим &осталим случајевима:"

#: askMe.label
msgid "Ask me"
msgstr "Питај ме"

#: alwaysSend.label
msgid "Always send"
msgstr "Увек шаљи"

#: neverSend.label
msgid "Never send"
msgstr "Не шаљи никад"
