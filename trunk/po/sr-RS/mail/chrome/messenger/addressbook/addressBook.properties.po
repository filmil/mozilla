# each of the attachments in the file|attachments menu:
#   ie: 1 file.txt
#       2 another file.txt
#: firstLastFormat
msgid "%S %S"
msgstr "%S %S"

#: newCardTitle
msgid "New Card"
msgstr "Нова картица"

# %S will be the card's display name
#: newCardTitleWithDisplayName
msgid "New Card for %S"
msgstr "Нова картица за %S"

#: editCardTitle
msgid "Edit Card"
msgstr "Уреди картицу"

# %S will be the card's display name
#: editCardTitleWithDisplayName
