#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Таблица стилова %1$S није била учитана, зато што њен MIME-тип (\"%2$S\") није \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Таблица стилова %1$S је била учитана, иако њен MIME-тип (\"%2$S\") није \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Неочекиван крај датотеке при потрази за %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Размак је задат као правило растављања."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Дефиниција је одбачена."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Пређи на следећу дефиницију."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Непозната особина '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Грешка при потрази за вредности особине '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Уместо завршетка вредности особине пронађено је '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "крај непознатог правила"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Непознато правило или грешка при растављању '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "сет словног низа у правилу @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Очекивани тип словног низа није пронађен '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "крај медија листе у правилу @import или @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Уместо ',' у медија листи пронађено је '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Уместо ознаке медија листе пронађено је '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Уместо URI у правилу @import пронађено је '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Пронађен је неочекивани '%1$S' под @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "крај правила @media или @-moz-document"

#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Уместо url(), url-prefix() или domain() у правилу @-moz-document пронађено је '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Уместо URI у правилу @-moz-document пронађено је '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "namespace префикс у правилу @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "namespace URI у правилу @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Неочекиван знак под @namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "затварање } блока дефиниције"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "затварање } неисправног сета правила"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Правило је игнорисано због лошег изборника."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' или '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Уместо ',' или '{' пронађено је '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Очекујем избор."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Додатни комбинатор."

#: PEClassSelEOF
msgid "class name"
msgstr "назив класе"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Уместо ознаке избора класе пронађено је'%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "тип елемента"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Уместо назива елемента или '*' пронађено је '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Непознат префикс '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "име атрибута"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Уместо ознаке имена атрибута пронађено је '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Уместо имена атрибута или назива простора пронађено је '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Уместо '|' пронађено је '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "део избора атрибута"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Неочекивани знак у избору атрибута: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "вредност атрибута"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' на крају избора атрибута"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Уместо ']' за прекид избора атрибута пронађено је '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Уместо ознаке или низа знакова за вредност избора атрибута пронађено је '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "назив псеудо-класе или псеудо-елемента"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Уместо ознаке псеудо-класе или псеудо-елемента пронађено је '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Пронађен је знак функције за не функцијску псеудо-класу или псеудо-елеменат, или на неком другом месту при читању '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Уместо псеудо-елемента пронађено је '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Псеудо-класа не може бити савладана са '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Псеудо-елеменат не може да буде негативан '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Псеудо-елеменат мора да користи \"::\" форму: '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Пронађен је крајњи знак после псеудо-елента, који мора да се налази на крају избора: '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Додатни псеудо-елеменат '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Непозната псеудо-класа или псеудо-елеменат '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "избор у оквиру негације"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Неисправни једноставни избор савлађује аргуменат псеудо-класе '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Недостаје затварајући ')' за савлађивање псеудо-класе '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Недостаје аргуменат за савлађивање псеудо-класе '%1$S'."

#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "аргуменат :lang избору"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Уместо ознаке параметра псеудо-класе пронађено је '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Недостаје затварајући ')' у lang псеудо-класи '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Недостаје аргуменат у lang псеудо-класи '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "изборник"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Уместо '{' на крају блока дефиниције пронађено је '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "боја"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Очекивана боја није пронађена '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "боја"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Уместо процента пронашао сам '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Уместо целог броја пронашао сам '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Уместо броја или процента у rgb() сам пронашао '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Уместо '%2$S' пронађено је '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "нијанса"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Уместо ',' пронађено је '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "обојеност"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "осветљење"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "провидност боје"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Уместо броја пронађен је '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Уместо ')' пронађено је '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' или '}' на крају дефиниције"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Уместо ':' пронађено је '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Очекивана дефиниција '%1$S' није пронађена."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "крај дефиниције"

#: PEImportantEOF
msgid "important"
msgstr "важно"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Уместо 'важно' пронађено је '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Уместо ';' за прекид дефиниције пронађено је '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Уместо ';' или '}' за прекид дефиниције пронађено је '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Покушај коришћења недоступне особине"

#: PECommentEOF
msgid "end of comment"
msgstr "крај коментара"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Пронађен је незатворени словни низ '%1$S'."
