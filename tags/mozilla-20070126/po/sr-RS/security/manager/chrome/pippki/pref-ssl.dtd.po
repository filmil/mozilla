#: SSLProtocolVersions.caption
msgid "SSL Protocol Versions"
msgstr "Верзије SSL протокола"

#: SSLWarnings.caption
msgid "SSL Warnings"
msgstr "SSL Упозорење"

#: SSLClientAuthMethod.caption
msgid "Client Certificate Selection"
msgstr "Избор сертификата клијента"

#: pref.ssl.title
msgid "Secure Sockets Layer (SSL)"
msgstr "Secure Sockets Layer (SSL)"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "Омогући SSL верзија 3"

#: enable.ssl3.accesskey
msgid "3"
msgstr "3"

#: enable.tls
msgid "Enable TLS"
msgstr "Омогући TLS"

#: enable.tls.accesskey
msgid "T"
msgstr "Н"

#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "&brandShortName; вас може обавестити о нивоу безбедности страна које сте прегледали. &brandShortName; ће издавати упозорења и затраживати прекид пред:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "учитавањем стране која подржава шифровање"

#: warn.enteringsecure.accesskey
msgid "L"
msgstr "L"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "учитавањем стране која користи слабо шифровање"

#: warn.enteringweak.accesskey
msgid "o"
msgstr ""

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "прослеђивања података формулара са нешифроване на нешифровану страну"

#: warn.insecurepost.accesskey
msgid "S"
msgstr "з"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "приступом страници која подржава шифровање"

#: warn.leavingsecure.accesskey
msgid "a"
msgstr "д"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "преусмеравањем са једне зашифроване стране на другу"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "преусмеравањем са једне зашифроване стране на незашифровану"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "преглед стране са измешаним садржајем"

#: warn.viewmixed.accesskey
msgid "V"
msgstr "Н"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "Одредите како &brandShortName; ће бирати сертификат, ако га сајт затљажи:"

#: certselect.auto
msgid "Select Automatically"
msgstr "Самостално бирај"

#: certselect.auto.accesskey
msgid "A"
msgstr "П"

#: certselect.ask
msgid "Ask Every Time"
msgstr "Питај сваки пут"

#: certselect.ask.accesskey
msgid "E"
msgstr "E"
